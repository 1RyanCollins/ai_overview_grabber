{
  "manifest_version": 3,
  "name": "AI Overview Link Grabber",
  "version": "1.0",
  "description": "Extracts AI overview links from Google search results.",
  "permissions": ["activeTab", "scripting", "downloads"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
